Private Sub Form_Load()

    Dim sSql As String

    m_AjoutLangue = False
    Me.txtBdd.SetFocus
    Me.txtBdd = "Sélectionnez une base..."

    sSql = "0;" & "<Tout>" & ";" & "1;" & C_FORM & ";" & "2;" & C_REPORT & ";" & "3;" & C_TABLE_LOCALE & ";" & "4;" & C_TABLE_LINK
    Me.zlTypes.RowSource = sSql

    Set ScanTxt = New C_TradScanText        '// Initialisation des classes.

End Sub

'----------------------------------------------------------------
