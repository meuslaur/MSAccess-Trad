'// ################################ PRIVATE SUB/FUNC ####################################

'// Applique les valeurs par défaut.
Private Sub RazForm(Optional bActive As Boolean)

    Me.txtBdd.SetFocus
    Me.txtBdd = "Sélectionnez un base..."
    Me.txtBddSauve = vbNullString
    Me.zl_Apps = Null
    Me.lstObjets = Null
    
    Me.lstObjets.RowSource = vbNullString

    Me.cmdCloseBd.Visible = bActive
    Me.cmbSelectBdd.Visible = Not bActive
    Me.cmd_ActuLstObjets.Enabled = bActive
    Me.zl_Apps.Enabled = Not bActive

End Sub

Private Sub ExtraireLangueCours()
    Dim lLang As Long
    Dim sSql As String
    Dim Rst As DAO.Recordset
    
    lLang = Application.LanguageSettings.LanguageID(msoLanguageIDUI)
    'lLang = 2070 'TODO: Test
    sSql = "SELECT T_Langues.* FROM T_Langues " & _
           "WHERE (((T_Langues.LangueCode)=" & lLang & "));"
    Set Rst = CurrentDb.OpenRecordset(sSql, dbOpenSnapshot)
    If (Rst.RecordCount <> 0) Then
        Me.lbl_Langue.Caption = Rst.Fields("LangueNom").Value
        Me.img_Langue.Picture = CurrentProject.Path & "\Flags\" & Rst.Fields("LangueCode").Value & ".png"
    Else
        Me.lbl_Langue.Caption = "??? voir table T_Langues"
        Me.img_Langue.Picture = CurrentProject.Path & "\Flags\" & "_Nolang.png"
    End If

    Rst.Clone
    Set Rst = Nothing

End Sub
