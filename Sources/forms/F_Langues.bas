'@Folder("Form")
' ------------------------------------------------------
' Name     : Form_T_Langues
' ------------------------------------------------------
' Kind     : Document VBA
' Purpose  : Ajoute d'une langue dans la table T_Langues
' Usage    : Si OpenArg null on sort.
' Author   : Laurent
' Date     : 03/07/2022 - 14:37
' DateMod  :
' ------------------------------------------------------
Option Compare Database
Option Explicit

'//::::::::::::::::::::::::::::::::::    VARIABLES      ::::::::::::::::::::::::::::::::::
'//:::::::::::::::::::::::::::::::::: END VARIABLES ::::::::::::::::::::::::::::::::::::::

'// ################################ PRIVATE SUB/FUNC ####################################
'// ################################# END PRIV. SUB/FUNC #################################

'//&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&     EVENTS        &&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&
Private Sub Form_Open(Cancel As Integer)
    If ((Len(Me.OpenArgs) = 0) Or (IsNull(Me.OpenArgs))) Then
        Cancel = True
        Exit Sub
    End If
End Sub
'----------------------------------------------------------------
' Procedure Nom   : Form_Load
' ----------------------------------------------------------------
' Sujet           : Création visilisation des langues
' Procedure Kind  : Sub
' Procedure Access: Private
' Objectif        :
'
' Author  : Laurent
' Date    : 05/07/2022 - 06:10
' DateMod :
' ----------------------------------------------------------------
Private Sub Form_Load()
On Error GoTo ERR_Form_Load

    Dim lCode As Long

    If Me.NewRecord Then
        '// Ajout d'une langue.
        Me.LangueCode.Enabled = True
        Me.LangueCode.SetFocus
    Else
        '// Affiche la langue.
        Me.AllowAdditions = False
        lCode = Val(Nz(Me.OpenArgs(), 0))
        Me.Filter = "LangueCode=" & lCode
        Me.FilterOn = True

        If (Len(Me.LangueImage) <> 0) Then
            Me.imgLangue.Picture = CurrentProject.Path & DOS_DRAP & Me.LangueImage
            Me.imgLangue00.Visible = False
        End If
    End If

SORTIE_Form_Load:
    Exit Sub

ERR_Form_Load:
    MsgBox "L’erreur suivante s’est produite" & vbCrLf & vbCrLf & _
           "Erreur N°: " & Err.Number & vbCrLf & _
           "Source : Form_F_Langues.Form_Load" & vbCrLf & _
           "Description: " & Err.Description & _
           Switch(Erl = 0, vbNullString, Erl <> 0, vbCrLf & "Line No: " & Erl), _
           vbOKOnly + vbCritical, "Erreur survenue !"
    Resume SORTIE_Form_Load
End Sub

Private Sub Form_AfterInsert()
'// Indique au form F_Main qu'une nouvelle langue à été ajouter.
    Forms.Item("F_Main").AddLangue Me.LangueCode
End Sub

Private Sub cmbSelectImage_Click()
    Dim sImgSel  As String
    Dim sImg     As String
    Dim sImgNom  As String
    Dim sDosFlag As String

    '// Séléction de la base à utiliser.
    sDosFlag = CurrentProject.Path & DOS_DRAP
    sImgSel = OuvreBoite("Image png", "*.png", "Sélection de l'image du drapeau.", sDosFlag, FD_TypeFilePicker)
    If (sImgSel = vbNullString) Then Exit Sub

    sImg = FSOGetFileName(sImgSel)
    sImgNom = FSOGetBaseName(sImgSel)

    '// Vérifier si le nom correspond au code de la langue.
    If (Me.LangueCode <> Val(sImgNom)) Then
        MsgBox "Le nom de l'image doit correspondre au Code de la langue.", vbCritical, "Image de la langue"
        Exit Sub
    End If

    Me.imgLangue00.Visible = False
    Me.imgLangue.Picture = sImgSel
    Me.LangueImage = sImg

End Sub


Private Sub cmdAnnuler_Click()
    If Me.Dirty Then Me.Undo
    DoCmd.Close
End Sub

Private Sub cmdValider_Click()
    DoCmd.Close
End Sub
